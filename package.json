{
  "name": "cohere-vscode",
  "displayName": "cohere-vscode",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:cohere-vscode.helloWorld"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cohere-vscode.helloWorld",
        "title": "Hello World",
        "category": "Cohere"
      },
      {
        "command": "cohere-vscode.doSomething",
        "title": "Do Something",
        "category": "Cohere"
      },
      {
        "command": "cohere-vscode.helloWebviews",
        "title": "Webviews",
        "category": "Cohere"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.75.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vscode/test-electron": "^2.2.2",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "mocha": "^10.1.0",
    "prettier": "^2.8.4",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "cohere-ai": "^5.1.0",
    "eventsource-parser": "^0.1.0",
    "openai": "^3.1.0"
  }
}
